{
    "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "7.11.0",
        "generators": {
            "api": {
                "generatorName": "typescript-axios",
                "output": "./src/api/axios-client",
                "inputSpec": "https://api.setka-rtu.ru/openapi/v1.json",
                "additionalProperties": {
                    "supportsES6": true,
                    "snapshot": true,
                    "withInterfaces": true,
                    "withSeparateModelsAndApi": true,
                    "apiPackage": "api",
                    "modelPackage": "models"
                }
            },
            "admin-api": {
                "generatorName": "typescript-axios",
                "output": "./src/api/axios-admin",
                "inputSpec": "https://api-admin.setka-rtu.ru/openapi/v1.json",
                "additionalProperties": {
                    "supportsES6": true,
                    "snapshot": true,
                    "withInterfaces": true,
                    "withSeparateModelsAndApi": true,
                    "apiPackage": "api",
                    "modelPackage": "models"
                }
            }
        }
    }
}
